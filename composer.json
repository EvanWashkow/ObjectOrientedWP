{
    "name":        "evanwashkow/object-oriented-wordpress",
    "description": "Access all of WordPress in an object-oriented way",
    "version":     "0.2.0",
    "autoload": {
        "classmap": [
            "WordPress.php"
        ],
        "psr-4": {
            "WordPress\\": [
                "WordPress",
                "WordPress/libraries"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/EvanWashkow/php-libraries"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/EvanWashkow/php-utilities"
        }
    ],
    "require": {
        "php": ">=5.4",
        "evanwashkow/php-libraries": ">=0.1.2",
        "evanwashkow/php-utilities": ">=0.2.0"
    },
    "authors": [
        {
            "name":  "Evan Washkow",
            "email": "epwashkow@gmail.com",
            "role":  "Developer"
        }
    ],
    "license": "GPL-3.0-or-later"
}
